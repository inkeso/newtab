{
  "manifest_version": 3,
  "name": "Usefull New Tab",
  "description": "A more usefull new tab page, showing search keywords and rectly closed tabs.",
  "version": "1.0",
  "developer": {
    "name": "Inkeso",
    "url": "https://github.com/inkeso/newtab"
  },
  "permissions": ["management", "search", "sessions", "tabs"],
  "chrome_url_overrides": { "newtab": "index.html" },
  "browser_specific_settings": {
    "gecko": {
      "id": "usefull-new-tab@inkeso",
      "strict_min_version": "100.0"
    }
  }
}
